@model List<KeyValuePair<int, string>>
<div class="form-group">
    <select class="form-control" v-model="DictType">
        <option value="">全部类型</option>
        @{
            if (Model.Any()) {
                foreach (var item in Model) {
                    <option value="@item.Key">@item.Value</option>
                }
            }
        }
    </select>
</div>