@model List<PF.Core.Entity.Sys_dict>
<div class="form-group">
    <select class="form-control" v-model="StrategyId" size="1" style="width:200px">
        <option value="">请选择</option>
        @{
            if (Model.Any()) {
                foreach (var item in Model) {
                    <option value="@item.DictId">@item.DictName</option>
                }
            }
        }
    </select>
</div>
<textarea v-model="DicRemark" class="form-control" rows="3" style="display:none"></textarea>

